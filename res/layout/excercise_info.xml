<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/LinearLayoutEI"
	android:orientation="vertical" 
	android:layout_width="fill_parent" 
	android:layout_height="fill_parent" 
	xmlns:android="http://schemas.android.com/apk/res/android">
<TextView android:text="@string/times" android:id="@+id/text_times" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
<EditText android:text="" android:id="@+id/editText_times" android:layout_width="wrap_content" android:layout_height="wrap_content"></EditText>
<TextView android:text="@string/weight" android:id="@+id/text_weight" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
<EditText android:text="" android:id="@+id/editText_weight" android:layout_width="wrap_content" android:layout_height="wrap_content"></EditText>
<Button android:text="@string/done" android:id="@+id/button_ei_done" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
</LinearLayout>